{
  "name": "example",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "type": "module",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "npm run lint --workspaces --if-present",
    "typeorm": "./node_modules/typeorm/cli.js --dataSource dist/sources/typeorm/config.js",
    "sequelize": "yarn sequelize-cli --config src/sources/sequelize/config.js",
    "knex": "dotenv -c '.env' -- npx knex --knexpath dist/sources/objectionjs/index.js --knexfile dist/sources/objectionjs/knexfile.cjs",
    "mikroorm": "node --experimental-specifier-resolution=node --loader ts-node/esm ./node_modules/.bin/mikro-orm",
    "migration:up": "yarn sequelize db:migrate && yarn typeorm migration:run && yarn mikroorm migration:up && yarn prisma migrate dev --schema prisma/schema.prisma && yarn knex migrate:latest",
    "migration:up:production": "yarn sequelize db:migrate && yarn typeorm migration:run && yarn mikro-orm migration:up && yarn prisma migrate deploy --schema prisma/schema.prisma",
    "docker:up": "docker-compose -f docker-compose.yaml up -d",
    "start:dev": "nodemon",
    "start": "node dist/index.js",
    "build": "npm run build --workspaces --if-present",
    "build:watch": "tsc -w",
    "postgres:clean": "node dist/scripts/truncate-postgres",
    "mysql:clean": "prisma migrate reset --force && prisma migrate deploy",
    "mongodb:clean": "node dist/scripts/truncate-mongodb",
    "db:clean": "yarn postgres:clean && yarn mysql:clean && yarn mongodb:clean",
    "seed:typeorm": "node dist/sources/typeorm/seeds/run",
    "seed:sequelize": "node dist/sources/sequelize/seeds/run",
    "seed:mongoose": "node dist/sources/mongoose/seeds/run",
    "seed:mikro-orm": "node dist/sources/mikroorm/seeds/run",
    "seed:prisma": "node dist/sources/prisma/seeds/run",
    "seed:all": "yarn seed:typeorm && yarn seed:sequelize && yarn seed:mongoose && yarn seed:mikro-orm && yarn seed:prisma",
    "clean": "rm -rf ./node_modules && rm -rf ./packages/cypress/node_modules && rm -rf ./packages/eslint-plugin-automation/node_modules && rm -rf ./packages/eslint-plugin-automation/lib && rm -rf ./packages/playwright/node_modules && rm -rf ./packages/scripts/node_modules && rm -rf ./packages/scripts/lib",
    "copy-theme": "copyfiles -u 1 src/themes/**/theme.bundle.js src/themes/**/*.css dist/",
    "ci:lint": "npm run ci:lint --workspaces --if-present",
    "ci:tsc": "npm run ci:tsc --workspaces --if-present",
    "playwright:ci:gated:stable:test": "npm run ci:test:stable -w packages/playwright",
    "playwright:ci:gated:unstable:test": "npm run ci:test:unstable -w packages/playwright",
    "playwright:ci:staging:stable:test": "npm run ci:test:stable:stg-excluded -w packages/playwright",
    "playwright:ci:staging:unstable:test": "npm run ci:test:unstable:stg-excluded -w packages/playwright",
    "playwright:setup": "npm run setup -w packages/playwright",
    "playwright:start": "npm run start -w packages/playwright",
    "playwright:start:debug": "npm run playwright:start -- -- --debug",
    "playwright:headless": "npm run test -w packages/playwright -- --workers 4 --reporter=html",
    "precommit": "npm run precommit --workspaces --if-present",
    "preinstall": "npm run build --workspaces --if-present",
    "scripts:generate-data-model-json": "scripts generate-data-model-json",
    "scripts:generate-tests-report": "scripts generate-tests-report",
    "scripts:reopen-skipped-tickets": "scripts reopen-skipped-tickets",
    "scripts:sync-tags": "scripts sync-tags",
    "scripts:verify-tickets-existence": "scripts verify-tickets-existence",
    "tsc": "npm run tsc --workspaces --if-present"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./dist/sources/mikroorm/config.js",
      "./src/sources/mikroorm/config.ts"
    ]
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@adminjs/express": "^6.0.0",
    "@adminjs/fastify": "^4.0.0",
    "@adminjs/hapi": "^7.0.0",
    "@adminjs/koa": "^4.0.0",
    "@adminjs/mikroorm": "^3.0.0",
    "@adminjs/mongoose": "^4.0.0",
    "@adminjs/objection": "^2.0.0",
    "@adminjs/passwords": "^4.0.0",
    "@adminjs/prisma": "^4.0.0",
    "@adminjs/sequelize": "^4.0.0",
    "@adminjs/themes": "^1.0.0",
    "@adminjs/typeorm": "^5.0.0",
    "@faker-js/faker": "^7.6.0",
    "@hapi/boom": "^10.0.1",
    "@hapi/cookie": "^12.0.1",
    "@hapi/hapi": "^21.3.0",
    "@koa/router": "^12.0.0",
    "@mikro-orm/cli": "^5.6.15",
    "@mikro-orm/core": "^5.6.15",
    "@mikro-orm/migrations": "^5.6.15",
    "@mikro-orm/nestjs": "^5.1.7",
    "@mikro-orm/postgresql": "^5.6.15",
    "@nestjs/common": "^9.3.12",
    "@nestjs/core": "^10.3.8",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/typeorm": "^10.0.2",
    "@prisma/client": "^4.11.0",
    "adminjs": "^7.0.9",
    "ajv-formats": "^2.1.1",
    "argon2": "^0.30.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-pg-simple": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "fastify-static": "^4.7.0",
    "i18next": "^22.4.13",
    "knex": "^2.4.2",
    "koa": "^2.14.2",
    "koa2-formidable": "^1.0.3",
    "mongoose": "^7.0.2",
    "nodemon": "^3.1.0",
    "objection": "^3.0.1",
    "pg": "^8.10.0",
    "prisma": "^4.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "sequelize": "^6.29.3",
    "sequelize-cli": "^6.6.0",
    "tslib": "^2.5.0",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.1",
    "@types/argon2": "^0.15.0",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/cors": "^2.8.13",
    "@types/express-session": "^1.17.7",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.6",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  }
}
